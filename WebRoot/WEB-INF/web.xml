    <servlet-name>DddSerselet</servlet-name>
    <servlet-class>DddSerselet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>loginSerselet</servlet-name>
    <servlet-class>loginSerselet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
