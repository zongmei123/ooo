    <servlet-name>readcookieSerselect</servlet-name>
    <servlet-class>readcookieSerselect</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>DddSerselet</servlet-name>
    <url-pattern>/dd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginSerselet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>cgSerselet</servlet-name>
    <url-pattern>/cg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dddSerselet</servlet-name>
