  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Writcookie</servlet-name>
    <url-pattern>/cookie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>readcookieSerselect</servlet-name>
    <url-pattern>/readc</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
