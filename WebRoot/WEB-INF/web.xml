    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>dddSerselet</servlet-name>
    <servlet-class>dddSerselet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Writcookie</servlet-name>
    <servlet-class>Writcookie</servlet-class>
